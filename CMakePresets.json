{
    "version": 8,
    "configurePresets": [
        {
            "name": "clang-preset",
            "displayName": "Clang 20.1.7 arm64-apple-darwin24.5.0",
            "description": "Using compilers: C = /opt/homebrew/opt/ccache/libexec/clang, CXX = /opt/homebrew/opt/ccache/libexec/clang++",
            "binaryDir": "${sourceDir}/out/build/${presetName}",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}",
                "CMAKE_C_COMPILER": "/opt/homebrew/opt/ccache/libexec/clang",
                "CMAKE_CXX_COMPILER": "/opt/homebrew/opt/ccache/libexec/clang++",
                "CMAKE_BUILD_TYPE": "Debug"
            }
        }
    ]
}